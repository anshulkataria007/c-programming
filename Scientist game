#include <iostream>
using namespace std;
int main() {
	int a,b,c,sum=0;
  cin>>a;
  c=a;
  while(c!=0)
  {
    b=c%10;
    sum=sum+(b*b*b);
    c=c/10;
  }
  if(sum==a)
  {
    cout<<"Give to Scientist Armstrong"<<endl;
  }
  else
  {
    cout<<"Dont Give to Scientist Armstrong"<<endl;
  }
  
	return 0;
}
