#include <iostream>
using namespace std;
class A
{
  public:
  int a;
};
class B:public A
{
public:
B()
{cin>>a;
}
};
class C
{
  public:
  int b;
  C()
  { cin>>b;
  }
};
class D:public B,public C
{
public:
void sum()
{cout<<"Sum= "<<a+b;
}
};
int main()
{
  D d;
  B c();
  C e();
  d.sum();
  return 0;
}
